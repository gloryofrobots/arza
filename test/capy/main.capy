fun map(seq, acc, fn):
    if seq.is_empty():
        acc
    else:
        head = seq.head()
        tail = seq.tail()
        val = fn(head)
        acc = acc.append(val)
        map(tail, acc, fn)

fun loop(counter):
//    print(counter)
    if counter > 0:
        loop(counter - 1)


