from arza:lang:_io import (write, stdout)
import seq
import string


fun _write_list_items(args, func, file, ends) =
    let
        s1 = string:join_with(func, args, " ")
        s2 = s1 ++ ends

    in
        write(s2, file)


fun printr(...args) =
    let
        out = stdout()
    in
        _write_list_items(args, repr, out, "\n")


fun print(...args) =
    print_list(args)


fun print_list(items) =
    let
        out = stdout()
    in
        _write_list_items(items, str, out, "\n")


let
    p = print
    pr = printr

