import seq
import io
import list
import string

"""
Nine billion names of God the Integer
see https://rosettacode.org/wiki/9_billion_names_of_God_the_integer
"""

fun nbn_1() =
    let fun _loop(n, g) =
        if g == 1 or n < g then
           1
        else
            seq:foldl(
                (fun(q, res) =
                    let x =
                        if q > n - g then
                             0
                        else
                            _loop(n-g, q)
                    in
                        res + x
                ),
                1,
                list:range(2, g)
            )

    in
        string:join(
            "\n",
            seq:map(
                fun (n) =
                    string:join(
                       " ",
                       seq:map(
                            g -> _loop(n, g),
                            list:range(1, n)
                       )
                    ),
                 list:range(1, 25)
             )
        )


fun nbn_2 () =
    string:join(
        "\n",
        seq:map(
            fun(n) =
                string:join(
                   " ",
                   seq:map(
                        (fun (g) =
                            let
                                fun _loop (n, g) =
                                    if g == 1 or n < g then 1
                                    else
                                        seq:foldl(
                                            (fun (q, res) =
                                                if q > n - g  then
                                                    res
                                                else
                                                    res + _loop(n-g, q)
                                            ),
                                            1,
                                            list:range(2, g)
                                        )

                            in _loop(n, g)
                        ),
                        list:range(1, n)
                   )
                )
           ,
           list:range(1, 25)
        )
    )

fun run () =
    // io:p (nbn_1 ())
    io:p (nbn_2 ())