import lib.math

PL(__env__())

class Pair =
    fun __init__(self, first, second) =
        self.{first = first, second = second}

    fun __add__(self, other) =
        let f = self.first + other.first
        let s = self.second + other.second
        Pair(f, s)

    fun sum(self) =
        self.first + self.second


fun main() =

    let p = Pair(1, 2)
    PL(p.sum())
    p.{x = 1}
    PL(p.x)


    __env__()
