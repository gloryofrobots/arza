from lalan:lang:_io import (write, stdout)
import seq


fun _write_list_items(args, func, file) =
    let fun _walk(x) =
        (
            write(func(x), file)
            write(" ", file)
        )

    in seq:foreach(_walk, to_seq(args))


fun printr(...args) =
    let out = stdout()
    in (
        _write_list_items(args, repr, out)
        write("\n", out)
    )


fun print(...args) =
    let out = stdout()
    in (
        _write_list_items(args, str, out)
        write("\n", out)
    )

let (
    p = print
    pr = printr
)

