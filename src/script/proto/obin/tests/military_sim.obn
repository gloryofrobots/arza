trait Soldier
trait Civilian

generic attack

reify attack {
    (self of Soldier, target of Civilian) {
        print(self, "not killing a civilian", target)
    }
    (self of Soldier, target of Soldier) {
        print(self, "try killing soldier", target)
    }
}
