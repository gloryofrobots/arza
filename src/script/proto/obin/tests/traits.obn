object Human {
    __name__ = "Human"
    name = nil
    fn make_shit(self) {
        print("SHIT from ", self.name)
    }
}

object Insect {
    fn eat_human(self, h) {
        print("Eating ", h.name)
    }
}

object Alice(Human, Insect) {
    id = 42
    name = "Alice"
    object Bob(Human) {
        name = "Bob"
        fn hello(self) {
            print("I am Bob")
        }
    }
    fn greetings(self) {
        print("Hello from", self.name)
    }
    goNorth = fn (self) {
        print("I ", self.name, " go North")
    }
}

