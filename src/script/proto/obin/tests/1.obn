
A = [(1,2), (2,3,4), 5, [6,7,8]]
print(
match A:
    case [a,b,c]: (a, b, c) end
    case [a, b]: (a,b) end
    case (x, y, z): (x, y, z) end
    case [(1, X), Y, 5, [6,7, Z]]: (X, Y, Z) end
    case _: 123 end
end
)

