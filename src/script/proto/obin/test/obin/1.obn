
func main() ->
    print(match (1, (23, (24, 25)))
            case (Z, (G, 1)) ->  12 + Z
            case (1, 23) ->   12
            case (1, (23, X)) ->  (:third, X)
            case _ ->  nil
            end)

    A = [(1,2), (2,3,4), 5, [6,7,8]]
    print(match A
            case [a,b,c] ->  (a, b, c)
            case [a, b] ->  (a,b)
            case (x, y, z) ->  (x, y, z)
            case [(1, X), Y, 5, [6,7, Z]] ->  (X, Y, Z)
            case _ ->  123
            end)

    A = {name="Bob", surname=("Alice", "Dou"), age=42}
    print(A)

    print(match A
            case {name="Bob", surname="Alice"} ->  (name, surname)
            case {name="Bob2", age=42} ->  (name, age)
            case {name="Bob", surname=("Alice", "Dou"), age=42} ->  (surname, name, age + 100)
            case _ ->  42
            end)

    print(
    match A
        case {age=41, names} ->  (name, age, 0)
        case {name, age=42} ->  (name, age, 1)
        case {age=42} ->  (age, 2)
        case _ ->  42
        end)

    print(
        match (1, 2, 1)
            case (A, x, A)  -> (24, A)
            case (3, A) -> 42
            end)
end
