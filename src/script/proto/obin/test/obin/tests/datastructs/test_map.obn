load testit

def test ->
    m = {name = "Alice", :surname="Bob",
            :child={
                :little_name="Sweetie",
                :toy={
                    :model="SXR 123344:44332"
                }
             }
     }
    testit.assert_equal(m.name, "Alice")
    testit.assert_equal(m[:child][:toy][:model], "SXR 123344:44332")
    testit.assert_equal(m[:child][:toy][:model], m.child.toy.model)

    testit.assert_true(:name in m)
    testit.assert_true(:surname in m)
    testit.assert_true(:little_name in m.child)
    testit.assert_false(:serial in m.child.toy)
end
