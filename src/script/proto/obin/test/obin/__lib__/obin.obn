load tvar

@prefix(#-, #uminus)
@prefix(#+, #uplus)
@prefix(#~, #~)
@infixr(#|, #|, 35)
@infixr(#^, #^, 45)
@infixr(#&, #&, 45)
@infixl(#<, #<, 50)
@infixl(#>, #>, 50)
@infixl(#>=, #>=, 50)
@infixl(#<=, #<=, 50)
@infixl(#==, #==, 50)
@infixl(#!=, #!=, 50)
@infixl(#<<, #<<, 55)
@infixl(#>>, #>>, 55)
@infixl(#>>>, #>>>, 55)
@infixl(#+, #+, 60)
@infixl(#-, #-, 60)
@infixl(#%, #%, 65)
@infixl(#*, #*, 65)
@infixl(#\, #\, 65)

