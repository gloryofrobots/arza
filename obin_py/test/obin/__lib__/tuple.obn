import obin:lang:_tuple
import string
from datatype import CoreMixin

extend Tuple
    with Eq = CoreMixin
    with Str
        def str self ->
            if len self == 1 ->
                "(" ++ str (at 0 self) ++ ",)"
            else ->
                "(" ++ string:join_cast self ", " ++ ")"
    with Collection = CoreMixin
    with Sized = CoreMixin
    with Seqable
        def to_seq self -> _tuple:to_list self
    with Sliceable
        def slice first last self -> _tuple:slice first last self
        def drop count self -> _tuple:drop count self
        def take count self -> _tuple:take count self
