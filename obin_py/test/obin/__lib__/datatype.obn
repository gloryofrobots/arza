import obin:lang:_datatype
import derive


extend Datatype with
    trait derive:TEq
    trait derive:TStr
    trait derive:TRepr

extend Union
    def to_seq(self) -> _datatype:union_to_list self
    def lower_bound(self) -> fst(to_seq(self))

    def upper_bound(self) -> last(to_seq(self))

    trait derive:TUnionEq
    trait derive:TUnionCollection
    trait derive:TUnionSized
