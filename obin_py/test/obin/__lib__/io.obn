from obin:lang:_io import (write, stdout)
import seq


fun _write_list_items args func file ->
    fun _walk x ->
        file |> write (func x)
             |> write " "

    seq:foreach _walk (to_seq args)
    ()


fun printr ...args ->
    out = stdout ()
    _write_list_items args repr out
    write "\n" out
    ()


fun print ...args ->
    out = stdout ()
    _write_list_items args str out
    write "\n" out
    ()

p = print
pr = printr


