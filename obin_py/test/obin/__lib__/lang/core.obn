
fun lookup key default coll ->
    try
        at key coll
    catch
        | e of KeyError -> default
        | e -> throw e

fun not_elem x c -> not (elem x c)

fun identity x -> x

fun |> x f -> f x
fun <| f x -> f x
fun >> f g x -> g (f x)
fun << f g x ->  f (g x)

fun twice f -> f >> f
fun flip f x y -> f y x
fun even x -> mod x 2 == 0
fun odd x -> not (even x)
